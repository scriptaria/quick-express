  },
  "main": "build/core/bootstrap.js",
  "apidoc": {
    "title": "Quick Express - Documentation"
  },
  "scripts": {
    "build": "del-cli build && npm run doc && tsc",
    "database": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "doc": "apidoc -i src/ -o doc/",
    "generate": "ts-node src/core/cli.ts generate",
    "nodemon": "nodemon src/core/bootstrap.ts start dev",
    "serve": "node build/core/bootstrap.js start prod",
    "start": "npm run build && npm run serve",
    "test": "jest"
  },
  "dependencies": {
    "bcrypt": "3.0.6",
    "body-parser": "1.19.0",
    "boolean": "3.0.0",
    "change-case": "4.1.1",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "glob": "7.1.6",
    "jsonwebtoken": "8.5.1",
    "make-runnable": "1.3.6",
    "module-alias": "2.2.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.4",
    "sqlite3": "4.1.1",
    "typeorm": "0.2.22",
    "validate.js": "0.13.1"
  },
  "devDependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/cors": "2.8.6",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.2",
    "@types/glob": "7.1.1",
    "@types/jest": "24.0.25",
    "@types/jsonwebtoken": "8.3.5",
    "@types/module-alias": "2.0.0",
    "@types/node": "12.12.21",
    "@types/supertest": "2.0.8",
    "apidoc": "0.19.1",
    "colors": "1.4.0",
    "commander": "4.1.0",
    "del-cli": "3.0.0",
    "jest": "24.9.0",
    "nodemon": "2.0.2",
    "supertest": "4.0.2",
    "ts-jest": "24.2.0",
    "ts-node": "8.5.4",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.1",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "3.7.4"
  }
}